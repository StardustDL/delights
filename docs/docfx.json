    //   ],
    //   "dest": "api/extensions",
    //   "disableGitFeatures": false,
    //   "disableDefaultFilter": false
    // },
    // {
    //   "src": [
    //     {
    //       "files": [
    //         "src/Delights*/**.csproj",
    //         "src/modules/**.csproj"
