    //     }
    //   ],
    //   "dest": "api/delights",
    //   "disableGitFeatures": false,
    //   "disableDefaultFilter": false
    // }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/modulight/**.yml",
          "api/modulight/index.md"
        ]
      },
      {
        "files": [
          // "api/extensions/**.yml",
          "api/extensions/index.md"
        ]
      },
      {
        "files": [
          // "api/delights/**.yml",
          "api/delights/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "Delights",
      "_appFooter": "Copyright Â© StardustDL.",
      "_enableSearch": true,
      "_enableNewTab": true,
      "_disableContribution": true,
      "_gitContribute": {
        "repo": "https://github.com/StardustDL/delights",
        "branch": "master",
        "apiSpecFolder": "docs/apidoc"
      }
    },
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}