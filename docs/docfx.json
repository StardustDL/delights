{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/Modulight.Modules.*/**.csproj"
          ],
          "cwd": ".."
        }
      ],
      "dest": "api/modulight",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    },
    // {
    //   "src": [
    //     {
